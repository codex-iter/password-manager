{
    "manifest_version":2,
    "name":"PWStorage_Extension",
    "description":"Stores Passwords",
    "version":"1.0",
    "browser_action":{
        "default_popup":"interface.htm",
        "default_icon":"Extension.png"
    },
   "permissions": [
        "storage","activeTab"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["myscript.js"]
    }]}
